itial\" }}\n    >\n      <Modal.Dialog>\n        <Modal.Header>\n          <Modal.Title>Your bet</Modal.Title>\n        </Modal.Header>\n\n        <Modal.Body>\n          {error && <div>please enter team and amount</div>}\n          {!error && overUnder < 0 && (\n            <div>\n              If the {team} win or loses by less then {-overUnder} you will win{\" \"}\n              {amount} else you will lose {amount * ratio}\n            </div>\n          )}\n          {!error && overUnder > 0 && (\n            <div>\n              If the {team} win by more then {overUnder} you will win {amount}{\" \"}\n              else you will lose {amount * ratio}\n            </div>\n          )}\n          {!error && overUnder === 0 && (\n            <div>\n              If the {team} win you will win {amount} else you will lose{\" \"}\n              {amount * ratio}\n            </div>\n          )}\n          {uploaded && <h5 className=\"bet-uploaded\">Bet uploaded!</h5>}\n        </Modal.Body>\n\n        <Modal.Footer className=\"footer\">\n          {!uploaded && (\n            <Button\n              variant=\"secondary\"\n              onClick={() => {\n                setShowModal(false);\n              }}\n            >\n              Cancel\n            </Button>\n          )}\n          {!uploaded && !error && (\n            <Button variant=\"primary\" onClick={clickHandler}>\n              Upload\n            </Button>\n          )}\n        </Modal.Footer>\n      </Modal.Dialog>\n    </div>\n  );\n}\n\nexport default BetModal;\n","/Users/amirashlag/Betting-Game-project-FE/src/components/BetCard/BetCard.jsx",["111","112"],[],"/Users/amirashlag/Betting-Game-project-FE/src/pages/Login/Login.js",[],[],"/Users/amirashlag/Betting-Game-project-FE/src/components/Table/table.jsx",[],[],"/Users/amirashlag/Betting-Game-project-FE/src/pages/LoggedOut/LoginOrSignup.js",[],[],"/Users/amirashlag/Betting-Game-project-FE/src/components/WeeklyCalendar/WeeklyCalendar.jsx",["113"],[]