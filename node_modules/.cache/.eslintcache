Name=\"form-control\"\n                          placeholder=\"bet ratio\"\n                          value={ratio}\n                          onChange={(e) => {\n                            if (e.target.value <= 0) {\n                              setRatio(1);\n                            } else {\n                              setRatio(e.target.value);\n                            }\n                          }}\n                        />\n                      </div>\n                    </div>\n                  </div>\n                </form>\n              </motion.div>\n              <Button\n                variant=\"primary\"\n                className=\"submit-bet\"\n                onClick={() => {\n                  setShowModal(true);\n                }}\n              >\n                Upload bet\n              </Button>\n            </motion.div>\n          )}\n        </Modal.Dialog>\n        {showModal && (\n          <BetModal\n            className=\"bet-modal\"\n            ratio={ratio}\n            amount={amount}\n            team={team}\n            overUnder={overUnder}\n            setShowModal={setShowModal}\n            gameData={gameData}\n          />\n        )}\n      </div>\n    </div>\n  );\n}\n\nexport default GameCard;\n","/Users/amirashlag/Betting-Game-project-FE/src/pages/LoggedOut/Signup.js",[],[],"/Users/amirashlag/Betting-Game-project-FE/src/components/GameList/GameList.jsx",["100","101","102","103","104","105"],[],"import { useEffect, useState } from \"react\";\nimport GameCard from \"../GameCard/GameCard\";\nimport axios from \"axios\";\nimport \"./GameList.css\";\nimport { useNavigate } from \"react-router-dom\";\n\nexport default function GameList({ games }) {\n  const navigate = useNavigate();\n\n  return (\n    <div className=\"game-list\">\n      <h2 className={games.length ? \"gamelist-headline\" : \"gamelist-headline2\"}>\n        {games.length ? \"Game List\" : \"No Games\"}\n      </h2>\n      {games &&\n        